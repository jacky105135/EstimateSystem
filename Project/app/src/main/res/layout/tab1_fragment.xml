<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    android:id="@+id/r1"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toolbar">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/test"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_below="@+id/toolbar"
                android:background="@drawable/greenrack"
                android:backgroundTint="#f67280"
                android:paddingStart="150dp"
                android:text="測驗"
                android:textColor="@color/colorwhite"
                android:textSize="25sp"/>

            <TextView
                android:id="@+id/date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="10dp"
                android:text="日期: "
                android:textSize="22sp"
                android:layout_below="@+id/test"/>

            <TextView
                android:id="@+id/date1"
                android:layout_width="250dp"
                android:layout_height="30dp"
                android:layout_marginLeft="85dp"
                android:layout_marginTop="10dp"
                android:background="#fafafa"
                android:textSize="22sp"
                android:layout_below="@+id/test"/>

            <TextView
                android:id="@+id/position"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/date"
                android:layout_below="@+id/date"
                android:layout_marginTop="5dp"
                android:text="地點:"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/position1"
                android:layout_width="250dp"
                android:layout_height="30dp"
                android:layout_below="@+id/date1"
                android:layout_marginLeft="85dp"
                android:layout_marginTop="5dp"
                android:background="#fafafa"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/patient"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_below="@+id/position"
                android:background="@drawable/greenrack"
                android:backgroundTint="#f67280"
                android:paddingStart="150dp"
                android:text="病人"
                android:textColor="@color/colorwhite"
                android:textSize="25sp"
                android:layout_marginTop="10dp"/>

            <TextView
                android:id="@+id/sex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/patient"
                android:text="性別:"
                android:textSize="22sp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="30dp"/>

            <TextView
                android:id="@+id/sex1"
                android:layout_width="250dp"
                android:layout_height="30dp"
                android:layout_below="@+id/patient"
                android:layout_marginLeft="85dp"
                android:layout_marginTop="10dp"
                android:background="#fafafa"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/state"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/sex"
                android:layout_below="@+id/sex"
                android:text="身分:"
                android:textSize="22sp"
                android:layout_marginTop="5dp"/>

            <TextView
                android:id="@+id/state1"
                android:layout_width="250dp"
                android:layout_height="30dp"
                android:layout_below="@+id/sex1"
                android:layout_marginLeft="85dp"
                android:layout_marginTop="5dp"
                android:background="#fafafa"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/age"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/state"
                android:layout_below="@+id/state"
                android:text="年齡:"
                android:textSize="22sp"
                android:layout_marginTop="5dp"/>

            <TextView
                android:id="@+id/age1"
                android:layout_width="250dp"
                android:layout_height="30dp"
                android:layout_below="@+id/state1"
                android:layout_marginLeft="85dp"
                android:layout_marginTop="5dp"
                android:background="#fafafa"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/anal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/age"
                android:layout_below="@+id/age"
                android:text="診斷:"
                android:textSize="22sp"
                android:layout_marginTop="5dp" />

            <TextView
                android:id="@+id/anal1"
                android:layout_width="250dp"
                android:layout_height="30dp"
                android:layout_below="@+id/age1"
                android:layout_marginLeft="85dp"
                android:layout_marginTop="5dp"
                android:background="#fafafa"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/operation"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:layout_alignStart="@+id/anal"
                android:layout_below="@+id/anal1"
                android:layout_marginTop="5dp"
                android:text="處置操作:"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/operation1"
                android:layout_width="200dp"
                android:layout_height="30dp"
                android:layout_marginTop="5dp"
                android:layout_alignStart="@+id/anal1"
                android:layout_below="@+id/anal1"
                android:background="#fafafa"
                android:layout_marginLeft="50dp"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/set"
                android:layout_width="180dp"
                android:layout_height="35dp"
                android:layout_below="@+id/operation"
                android:background="@drawable/greenrack"
                android:backgroundTint="#f67280"
                android:paddingStart="40dp"
                android:text="評估項目"
                android:textColor="@color/colorwhite"
                android:textSize="25sp"
                android:layout_marginTop="10dp"/>

            <TextView
                android:id="@+id/score"
                android:layout_width="180dp"
                android:layout_height="35dp"
                android:layout_below="@+id/operation"
                android:background="@drawable/greenrack"
                android:backgroundTint="#f67280"
                android:paddingStart="55dp"
                android:text="表現"
                android:textColor="@color/colorwhite"
                android:textSize="25sp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="180dp"/>

            <TextView
                android:id="@+id/a"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/date"
                android:layout_below="@+id/set"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="10dp"
                android:text="1.醫療面談"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/a_score"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:layout_below="@+id/score"
                android:layout_marginLeft="220dp"
                android:layout_marginTop="10dp"
                android:background="#fafafa"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/date"
                android:layout_below="@+id/a"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="10dp"
                android:text="2.身體檢查"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/b_score"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:layout_below="@+id/a_score"
                android:layout_marginLeft="220dp"
                android:layout_marginTop="10dp"
                android:background="#fafafa"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/c"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/date"
                android:layout_below="@+id/b"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="10dp"
                android:text="3.操作技能"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/c_score"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:layout_below="@+id/b_score"
                android:layout_marginLeft="220dp"
                android:layout_marginTop="10dp"
                android:background="#fafafa"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/d"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/date"
                android:layout_below="@+id/c"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="10dp"
                android:text="4.諮商衛教"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/d_score"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:layout_below="@+id/c_score"
                android:layout_marginLeft="220dp"
                android:layout_marginTop="10dp"
                android:background="#fafafa"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/e"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/date"
                android:layout_below="@+id/d"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="10dp"
                android:text="5.臨床判斷"
                android:textSize="22sp" />


            <TextView
                android:id="@+id/e_score"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:layout_below="@+id/d_score"
                android:layout_marginLeft="220dp"
                android:layout_marginTop="10dp"
                android:background="#fafafa"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/f"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/date"
                android:layout_below="@+id/e"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="10dp"
                android:text="6.組織效能"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/f_score"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:layout_below="@+id/e_score"
                android:layout_marginLeft="220dp"
                android:layout_marginTop="10dp"
                android:background="#fafafa"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/g"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/date"
                android:layout_below="@+id/f"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="10dp"
                android:text="7.人道專業"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/g_score"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:layout_below="@+id/f_score"
                android:layout_marginLeft="220dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="#fafafa"
                android:textSize="22sp" />

        </RelativeLayout>
    </ScrollView>
</RelativeLayout>